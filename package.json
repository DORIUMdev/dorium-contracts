{
  "name": "dorium-contracts",
  "version": "0.1.0",
  "description": "Smart contracts for Dorium",
  "main": "index.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node index.ts\"",
    "format": "prettier --write \"./**/*.{js, ts, html, json, toml, yaml, yml, md}\"",
    "lint": "eslint . --ext .js,.ts"
  },
  "lint-staged": {
    "*.{html, json, toml, yaml, yml, md}": [
      "prettier --write"
    ],
    "*.{ts}": [
      "prettier --write",
      "eslint . --ext .js,.ts"
    ]
  },
  "husky": {
    "hooks": {
      "applypatch-msg": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@cosmjs/cli": "^0.25.4",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.25.2",
    "@cosmjs/crypto": "^0.25.4",
    "@cosmjs/proto-signing": "^0.25.2",
    "@cosmjs/stargate": "^0.25.2",
    "dotenv": "^9.0.2",
    "ramda": "^0.27.1",
    "typescript": "^4.3.5"
  }
}
